<resources>



    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
       <!-- <item name="colorPrimary">@color/colorPrimary</item>-->
       <!-- <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <style name="Widget.MaterialSheetFab.TabLayout" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
        <item name="tabMode">fixed</item>
        <item name="tabGravity">fill</item>
    </style>

    <style name="TextAppearance.MaterialSheetFab.Note" parent="">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/darker_gray</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TextAppearance.MaterialSheetFab.Note.Title" parent="TextAppearance.MaterialSheetFab.Note">
        <item name="android:textSize">17sp</item>
    </style>

    <style name="aaa">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ButtonStyle" parent="Base.TextAppearance.AppCompat.Widget.Button">
        <item name="android:layout_width">260dp</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="ButtonRippleStyle" parent="ButtonStyle">
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>
    <style name="ButtonRippleLessStyle" parent="ButtonStyle">
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
    </style>

</resources>
